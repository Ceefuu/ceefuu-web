<style>
.section-feature-grey{

    background-color:#fbfbfb;

}

.has-bg-img {
  background: url("hero.png") center center;
  background-size: cover;
}

.app-side .title,.app-side .subtitle{text-align:center;max-width:550px;margin:2rem auto}

.mx-auto {
    width: 100%;
    margin-left: auto !important;
    margin-right: auto !important;
}

.max-w-4 {
    max-width: 27.5rem;
}

.hero-body {
  font-family: "Nexa Bold", sans-serif;
}

.container{
    font-family: "Nexa Bold", sans-serif;
}

.columns.is-vcentered{-webkit-box-align:center;-ms-flex-align:center;align-items:center}

.title.is-narrow{margin-bottom:0;}

.py-6{padding-top:3rem !important;padding-bottom:1rem;}
.py-2 {
    text-align:left;
    padding-top: 0.5rem !important;
    padding-bottom: 0.5rem !important;
}

@media only screen and (min-width: 320px) and (max-width: 1024px) and (orientation: portrait) {

.b-centered-tablet-p {
    text-align: center;
    margin-left: auto;
    margin-right: auto;
    max-width: 1024px;
}

}
</style>

<% content_for :head do %>
  <meta name="turbolinks-cache-control" content="no-cache">

        <!-- Primary Meta Tags -->
        <meta name="title" content="Ceefuu - Build & Scale a Virtual Mentoring Business">
        <meta name="description" content="Ceefuu is a virtual mentoring business builder for creators and online educators. Share your knowledge and earn money on your schedule. Join Ceefuu.">

        <!-- Open Graph / Facebook -->
        <meta property="og:type" content="website">
        <meta property="og:url" content="https://www.ceefuu.com/">
        <meta property="og:title" content="Ceefuu - Build & Scale a Virtual Mentoring Business">
        <meta property="og:description" content="Ceefuu is a virtual mentoring business builder for creators and online educators. Share your knowledge and earn money on your schedule. Join Ceefuu.">
        <meta property="og:image" content="<%= image_url("ceefuu-og.png") %>">

        <!-- Twitter -->
        <meta property="twitter:card" content="summary_large_image">
        <meta property="twitter:url" content="https://www.ceefuu.com/">
        <meta property="twitter:title" content="Ceefuu - Build & Scale a Virtual Mentoring Business">
        <meta property="twitter:description" content="Ceefuu is a virtual mentoring business builder for creators and online educators. Share your knowledge and earn money on your schedule. Join Ceefuu.">
        <meta property="twitter:image" content="<%= image_url("ceefuu-og.png") %>">
<% end %>

<section class="hero is-medium has-bg-img">
    <div class="hero-body">
        <div class="container"><br>
        
            <h1 class="title has-text-white has-text-left has-text-weight-bold is-2 is-size-4-mobile is-spaced main-title">
                Build & Scale a Virtual<br>Mentoring Business
            </h1>
           
            <h2 class="subtitle has-text-white is-4 is-size-6-mobile mb-4 mx-auto max-w-5 has-text-left">
                Earn money by sharing your knowledge.
            </h2><br>
        

        <% if (!user_signed_in?) %>
        <div class="container has-text-left">
            <%= link_to  "Start mentoring", new_user_registration_path, class: "button is-white", data: { turbolinks: false } %>
        </div>
          
        <% else %>
        <div class="container has-text-left">
            <%= link_to 'Dashboard', dashboard_path, class: "button is-white is-medium m-t-10" %>
        <% end %>
        </div>

        </div>
    </div>
</section>
